{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "dotenv-cli": "^3.2.0",
    "react": "^0.0.0-experimental-f77c7b9d7",
    "react-dom": "^0.0.0-experimental-f77c7b9d7",
    "react-error-boundary": "^2.3.1",
    "react-relay": "^0.0.0-experimental-8058ef82",
    "react-relay-network-modern": "^4.7.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "relay-runtime": "^10.0.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:coverage": "yarn test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "update:schema": "dotenv -e .env.local yarn schema",
    "schema": "get-graphql-schema $REACT_APP_API_ENDPOINT > schema.graphql",
    "relay": "relay-compiler"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-relay": "^7.0.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/relay-runtime": "^10.0.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-relay": "^10.0.1",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.3.0",
    "relay-compiler": "^10.0.1",
    "relay-compiler-language-typescript": "^13.0.1",
    "relay-config": "^10.0.1"
  },
  "jest": {
    "watchPathIgnorePatterns": [
      ".graphql.ts",
      "polyfill.ts",
      "serviceWorker.ts",
      "theme.ts",
      "src/index.tsx"
    ],
    "coveragePathIgnorePatterns": [
      ".graphql.ts",
      "polyfill.ts",
      "serviceWorker.ts",
      "theme.ts",
      "src/index.tsx"
    ]
  }
}
